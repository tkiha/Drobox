= form_for ([@folder, @upfile]),{url: move_folder_upfile_path(@folder, @upfile)}  do |f|
  #current
    = f.label :現在のフォルダ：
    = link_to @folder.name, search_folder_path(@folder), {id: 'current_folder',class: 'search_folder',remote: true}

  #moveto_folder

  %hr

  #tree

  = hidden_field_tag :authenticity_token, form_authenticity_token

  %hr
  .actions
    = f.submit '移動する'

