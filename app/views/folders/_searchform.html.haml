= form_for ("#{param[:form_for]}"),{url: "#{param[:url]}"} do |f|
  #current
    = f.label :現在のフォルダ：
    = link_to @folder.name, search_folder_path(@folder), {id: 'current_folder',class: 'search_folder',remote: true}

  %div.to_folder

  %hr

  %div.tree

  = hidden_field_tag :authenticity_token, form_authenticity_token

  %hr
  .actions
    = f.submit "#{param[:title]}する"

